{
    "name": "miladtech/shoppingcart",
    "description": "MiladTech Laravel Shoppingcart",
    "keywords": ["laravel", "shoppingcart","miladtech"],
    "license": "MIT",
    "authors": [
        {
            "name": "Milad Mostafaei",
            "email": "info@milad-tech.com",
            "homepage": "https://milad-tech.com/"
        }
    ],
    "require": {
        "illuminate/support": "5.4.*||5.5.*||5.6.*||5.7.*||5.8.*||^6.0||^7.0||^8.0||^9.0 || ^10.0",
        "illuminate/session": "5.4.*||5.5.*||5.6.*||5.7.*||5.8.*||^6.0||^7.0||^8.0||^9.0 || ^10.0",
        "illuminate/events": "5.4.*||5.5.*||5.6.*||5.7.*||5.8.*||^6.0||^7.0||^8.0||^9.0 || ^10.0",
        "nesbot/carbon": "~1.20||^1.26.3||^2.0 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0||~6.0||~7.0||~8.0||~9.0 || ~10.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.4|^4.0|^5.0|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "MiladTech\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MiladTech\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "suggest": {
        "miladtech/notify": "Simple flash notifications for Laravel"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "MiladTech\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "MiladTech\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}
